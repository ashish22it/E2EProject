<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">

<listeners>
 <listener class-name="Acedemy.Listeners"/>
</listeners>
 
  <test name="Validating User login">
